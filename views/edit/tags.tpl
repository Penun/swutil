{{template "includes/edit/header.tpl"}}
<body>
	<div class="mainDiv" id="forwardMain" ng-style="{'transform': 'rotateX('+mCont.rotateDeg+'deg)', '-moz-transform': 'rotateX('+mCont.rotateDeg+'deg)', '-webkit-transform': 'rotateX('+mCont.rotateDeg+'deg)'}">
		<div class="page sw_back">
            <div style="grid-column: 2; grid-row: 2; display: grid; grid-template-columns: 60vw 1fr; overflow-y: auto; justify-items: center;">
                <span style="grid-column: 1;"><textarea style="width: 60vw;" readonly><img src="/static/img/success.png" alt="SUC" /></textarea></span><span style="grid-column: 1;"><img class="sw_back_s" src="/static/img/success.png" alt="SUC" /></span>
                <span style="grid-column: 1;"><textarea style="width: 60vw;" readonly><img src="/static/img/advantage.png" alt="ADV" /></textarea></span><span style="grid-column: 1;"><img class="sw_back_s" src="/static/img/advantage.png" alt="ADV" /></span>
                <span style="grid-column: 1;"><textarea style="width: 60vw;" readonly><img src="/static/img/triumph.png" alt="TRI" /></textarea></span><span style="grid-column: 1;"><img class="sw_back_s" src="/static/img/triumph.png" alt="TRI" /></span>
                <span style="grid-column: 1;"><textarea style="width: 60vw;" readonly><img src="/static/img/boost.png" alt="BOD" /></textarea></span><span style="grid-column: 1;"><img class="sw_back_s" src="/static/img/boost.png" alt="BOD" /></span>
                <span style="grid-column: 1;"><textarea style="width: 60vw;" readonly><img src="/static/img/difficulty.png" alt="DIF" /></textarea></span><span style="grid-column: 1;"><img class="sw_back_s" src="/static/img/difficulty.png" alt="DIF" /></span>
                <span style="grid-column: 1;"><textarea style="width: 60vw;" readonly><img src="/static/img/threat.png" alt="THR" /></textarea></span><span style="grid-column: 1;"><img class="sw_back_s" src="/static/img/threat.png" alt="THR" /></span>
                <span style="grid-column: 1;"><textarea style="width: 60vw;" readonly><img src="/static/img/dispair.png" alt="DIS" /></textarea></span><span style="grid-column: 1;"><img class="sw_back_s" src="/static/img/dispair.png" alt="DIS" /></span>
                <span style="grid-column: 1;"><textarea style="width: 60vw;" readonly><img src="/static/img/setBack.png" alt="SBD" /></textarea></span><span style="grid-column: 1;"><img class="sw_back_s" src="/static/img/setBack.png" alt="SBD" /></span>
                <span style="grid-column: 1;"><textarea style="width: 60vw;" readonly><img src="/static/img/forceP.png" alt="FP" /></textarea></span><span style="grid-column: 1;"><img class="sw_back_s" src="/static/img/forceP.png" alt="FP" /></span>
            </div>
		</div>
	</div>
</body>
</html>
